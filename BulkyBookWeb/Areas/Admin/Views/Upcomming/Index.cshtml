@{
}
@{
    Layout = "_Layout1";
}

<div class="container" style="margin-top:150px;">

    <div class="row mt-3">
        <div class="col-6">
            <h3 class="">Upcomming Game List</h3>
		</div>
        <div class="col-6 text-end">
               <a asp-area="Admin" asp-controller="Upcomming" asp-action="Upsert" class="btn text-white" style="background:#cda45e;border-radius:50px">
               <i class="bi bi-plus-circle"></i> &nbsp; Upload New Upcomming Game
			</a>
		</div>
	</div><hr />
   
    

<table  id="tblData"  class="table table-bordered table-striped display text-white" style="margin-top:20px!important">
    <thead style="background:#2c3034;">
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Category</th>
            <th>Date</th>
            <th class="text-center">Action</th>
     
            
		</tr>
	</thead>
   
</table>

</div>
@section Scripts{
    <script src="~/js/upcomming.js"></script>
} 

