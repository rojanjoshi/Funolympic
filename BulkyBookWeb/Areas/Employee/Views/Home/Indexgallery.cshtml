@model BulkyBook.Models.ViewModels.HomeVM


@{
    Layout = "_Layout1";
}


   <main id="main">






  





    <!-- ======= Gallery Section ======= -->
   <section id="gallery" class="gallery mt-5">

      <div class="container" data-aos="fade-up">
        <div class="section-title">
          <h2>Gallery</h2>
          <p>All photos from Olympic</p>
        </div>
      </div>

      <div class="container-fluid" data-aos="fade-up" data-aos-delay="100">

        <div class="row g-0">

          

        @foreach(var item in Model.IndexGallerylist)
                {
                         <div class="col-lg-3 col-md-4">
            <div class="gallery-item">
              <a href="@item.ImageUrl" class="gallery-lightbox" data-gall="gallery-item">
                <img src="@item.ImageUrl" alt="" class="img-fluid" style="width:400px;height:300px;">
              </a>
            </div>
          </div>
                }


        </div>
      
      </div>
    </section>
    <!-- End Gallery Section -->









    </main>
